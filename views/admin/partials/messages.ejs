<% if(info){ %>
	<div class="alert
		<% if(info.ok){ %> 
			alert-success 
		<% } else { %>
			alert-danger 
		<% } %>
    	alert-dismissible fade show" role="alert">
        <%= info.msg %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
    </div>
<% } %>




